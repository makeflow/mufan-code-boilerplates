import React, {FC} from 'react';
import {HashRouter, Route, Switch} from 'react-router-dom';

import {
  {{#each pages}}
  {{this.componentName}},
  {{/each}}
  NoMatch
  } from './pages';

export const APP: FC = () => {
  return (
    <HashRouter>
      <Switch>
        {{#each pages}}
        <Route path="/{{this.name}}" component={ {{this.componentName}} } />
        {{/each}}
        <Route path="*" component={NoMatch} />
      </Switch>
    </HashRouter>
  );
};
